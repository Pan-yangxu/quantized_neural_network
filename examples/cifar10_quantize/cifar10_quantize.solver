net: "examples/cifar10_quantize/cifar10_quantize.prototxt"
test_iter: 100
test_interval: 1000
gamma: 0.1
base_lr: 0.005
momentum: 0.9
weight_decay: 0.0001
# The learning rate policy
stepvalue: 30000
stepvalue: 50000
lr_policy: "multistep"
# Display every 200 iterations
display: 100
# The maximum number of iterations
max_iter:  60000
# snapshot intermediate results
snapshot: 20000
snapshot_prefix: "examples/cifar10_quantize/trained_models/cifar10"
# solver mode: CPU or GPU
solver_mode: GPU
